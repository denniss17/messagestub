<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://www.istandaarden.nl/ijw/2_3/basisschema/schema" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ijw="http://www.istandaarden.nl/ijw/2_3/basisschema/schema">
	<xs:annotation>
		<xs:appinfo>
			<ijw:standaard>ijw</ijw:standaard>
			<ijw:bericht>basisschema</ijw:bericht>
			<ijw:release>2.3</ijw:release>
			<ijw:versie>1.0</ijw:versie>
		</xs:appinfo>
	</xs:annotation>
	<xs:complexType name="CDT_Achternaam">
		<xs:annotation>
			<xs:documentation>De achternaam van een natuurlijk persoon, aangeduid als Naam met Voorvoegsel.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Achternaam" type="ijw:LDT_Naam">
				<xs:annotation>
					<xs:documentation>De achternaam van een persoon, indien nodig verkort weergegeven.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Voorvoegsel" type="ijw:LDT_Voorvoegsel" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Verzameling van een of meer voorzetsels/lidwoorden, die aan het significante deel van de achternaam van een persoon vooraf gaat en daar een onderdeel van is.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Adres">
		<xs:annotation>
			<xs:documentation>Gegevens voor de aanduiding van een adres.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Huis" type="ijw:CDT_Huis" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Aanduiding van een specifiek huis in een straat, onderdeel van het adres.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Postcode" type="ijw:LDT_Postcode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Aanduiding van de (inter)nationale postcode. Dit veld is geschikt voor zowel de Nederlandse als niet-Nederlandse postcodes.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Straatnaam" type="ijw:LDT_Straatnaam" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De officiele door de gemeente vastgestelde naam van een straat.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Plaatsnaam" type="ijw:LDT_Plaatsnaam" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De door de gemeente vastgestelde naam van een woonplaats.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LandCode" type="ijw:LDT_LandCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identificatie van een (historisch/actueel) land of gebiedsdeel.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_BedragMetDC">
		<xs:annotation>
			<xs:documentation>Samengesteld bedrag met DebetCredit indicatie.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Bedrag" type="ijw:LDT_Bedrag">
			</xs:element>
			<xs:element name="DebetCredit" type="ijw:LDT_DebetCredit">
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_BerichtIdentificatie">
		<xs:annotation>
			<xs:documentation>Naam of nummer en dagtekening ter identificatie van een totale aanlevering.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Identificatie" type="ijw:LDT_IdentificatieBericht">
				<xs:annotation>
					<xs:documentation>Naam of nummer, die ter identificatie aan een totale aanlevering kan worden meegegeven.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Dagtekening" type="ijw:LDT_Datum">
				<xs:annotation>
					<xs:documentation>De datum van verzending van het bericht.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Communicatie">
		<xs:annotation>
			<xs:documentation>Aanduiding over bijzondere vorm van communicatie die gebruikt dient te worden.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Vorm" type="ijw:LDT_Communicatievorm">
				<xs:annotation>
					<xs:documentation>Gecodeerde aanduiding van de bijzondere vorm van communicatie die gebruikt dient te worden.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Taal" type="ijw:LDT_Taal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De taal van de client waarin met de client wordt gecommuniceerd.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_DeclaratieFactuurBedragen">
		<xs:sequence>
			<xs:element name="DeclaratieFactuurTotaalBedrag" type="ijw:CDT_TotaalBedragMetDC">
				<xs:annotation>
					<xs:documentation>Het bedrag dat in zijn totaliteit gedeclareerd of gefactureerd is.</xs:documentation>
					<xs:documentation>Eindtotaal van de bedragen in element DeclaratieFactuurBedrag over alle clienten die in een bericht zijn opgenomen. Kan gebruikt worden als controlegegeven. Het element kan nul bevatten als resultante van debet- en creditbedragen nul is.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BtwTotaalBedrag" type="ijw:CDT_TotaalBedragMetDC" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Het totaal BTW-bedrag dat in rekening wordt gebracht.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_DeclaratieFactuurIdentificatie">
		<xs:annotation>
			<xs:documentation>Samengestelde DeclaratieFactuurgegevens over Declaratieperiode, Factuurnummer, Dagtekening en BTW.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DeclaratiePeriode" type="ijw:CDT_GeslotenPeriode">
				<xs:annotation>
					<xs:documentation>Eerste en laatste dag van een administratieve periode waarop de gegevensuitwisseling betrekking heeft.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DeclarantFactuurNummer" >
				<xs:annotation>
					<xs:documentation>DeclarantFactuurNummer dient uniek te zijn (uniek voor een periode van 5 jaar). Een factuurnummer mag hergebruikt wordt in geval van een technische afkeuring.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="ijw:LDT_IdentificatieBericht">
						<xs:pattern value="([a-zA-Z0-9])+"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="FactuurDagtekening" type="ijw:LDT_Datum">
				<xs:annotation>
					<xs:documentation>Datum van ondertekening van een declaratie door de declarant.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BtwIDNummer" type="ijw:LDT_BtwIDNummer" minOccurs="0">
			</xs:element>
			<xs:element name="ValutaCode" type="ijw:LDT_ValutaCode">
				<xs:annotation>
					<xs:documentation>Valuta waarin alle bedragen zijn uitgedrukt.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Geboortedatum">
		<xs:annotation>
			<xs:documentation>Datum waarop een natuurlijk persoon geboren is.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Datum" type="ijw:LDT_Datum">
				<xs:annotation>
					<xs:documentation>Datum waarop een persoon geboren is.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DatumGebruik" type="ijw:LDT_DatumGebruik" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicator die aangeeft welk deel van de voorafgaande datum gebruikt mag worden.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_GeslotenPeriode">
		<xs:annotation>
			<xs:documentation>Tijdsperiode met zowel begin- als einddatum</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Begindatum" type="ijw:LDT_Datum">
				<xs:annotation>
					<xs:documentation>De begindatum van de periode.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Einddatum" type="ijw:LDT_Datum">
				<xs:annotation>
					<xs:documentation>De einddatum van de periode.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Grondslagen">
		<xs:annotation>
			<xs:documentation>De verzameling van gecodeerde aanduidingen die aangeven wat ten grondslag ligt aan het indicatiebesluit.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Grondslag01" type="ijw:LDT_Grondslag">
				<xs:annotation>
					<xs:documentation>Gecodeerde aanduiding van wat als eerste ten grondslag ligt aan het indicatiebesluit met betrekking tot de zorg voor een client.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Grondslag02" type="ijw:LDT_Grondslag" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Gecodeerde aanduiding van wat als tweede ten grondslag ligt aan het indicatiebesluit met betrekking tot de zorg voor een client.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Huis">
		<xs:annotation>
			<xs:documentation>Aanduiding van een specifiek huis in een straat, onderdeel van het adres.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Huisnummer" type="ijw:LDT_Huisnummer">
				<xs:annotation>
					<xs:documentation>De numerieke aanduiding zoals de gemeente die aan het object heeft toegekend.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Huisletter" type="ijw:LDT_Huisletter" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Een alfabetisch teken achter het huisnummer zoals dit door het gemeentebestuur is toegekend.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="HuisnummerToevoeging" type="ijw:LDT_HuisnummerToevoeging" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Die letters of tekens die noodzakelijk zijn om, naast huisnummer en -letter, de brievenbus te vinden.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AanduidingWoonadres" type="ijw:LDT_AanduidingWoonadres" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De aanduiding die wordt gebruikt voor adressen die niet zijn voorzien van de gebruikelijke straatnaam en huisnummeraanduidingen.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_IngediendeBedragPrestatie">
		<xs:sequence>
			<xs:element name="GeleverdVolume" type="ijw:LDT_Volume">
				<xs:annotation>
					<xs:documentation>Hoeveelheid verricht product in een prestatieperiode. Altijd vullen in relatie met Eenheid.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Eenheid" type="ijw:LDT_Eenheid">
				<xs:annotation>
					<xs:documentation>De eenheid waarin de zorg/behandeling wordt uitgedrukt. Wordt gevuld met een Eenheid die hoort bij het GeleverdVolume.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Verrekenpercentage" type="ijw:LDT_Percentage" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Berekeningsfactor over het normtarief voor het feitelijk gebruikte aandeel (event. in tijd) aan te geven in de declaratie van kosten, honorarium of verrichting.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProductTarief" type="ijw:LDT_Bedrag" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Tarief van een individueel product.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BerekendBedrag" type="ijw:CDT_BedragMetDC" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Het berekend bedrag is het bedrag dat de zorgverlener wil ontvangen. Berekend bedrag wordt alleen gevuld indien gemeente en aanbieder dit hebben vastgelegd in onderlinge afspraken.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BtwVrijstellingIndicatie" type="ijw:LDT_BtwVrijstellingIndicatie" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicatie die aangeeft of sprake is van BTW-vrijstelling.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BtwPercentage"  minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="ijw:LDT_Percentage">
						<xs:maxInclusive value="9999"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="BtwBedrag" type="ijw:CDT_BedragMetDC" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Het bedrag aan BTW dat in rekening wordt gebracht.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DeclaratieFactuurBedrag" type="ijw:CDT_BedragMetDC">
				<xs:annotation>
					<xs:documentation>Het bedrag dat gedeclareerd of gefactureerd wordt.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Omvang">
		<xs:annotation>
			<xs:documentation>Aanduiding van de omvang van de te leveren of geleverde ondersteuning, uitgedrukt in Volume, Eenheid en Frequentie.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Volume" type="ijw:LDT_Volume">
				<xs:annotation>
					<xs:documentation>Aanduiding van de mate van zorg betreffende een product.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Eenheid" type="ijw:LDT_Eenheid">
				<xs:annotation>
					<xs:documentation>Aanduiding van de eenheid waarin het volume is uitgedrukt.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Frequentie" type="ijw:LDT_Frequentie">
				<xs:annotation>
					<xs:documentation>Aanduiding van de frequentie waarmee een ondersteuningsproduct geleverd wordt.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_OpenPeriode">
		<xs:annotation>
			<xs:documentation>Aanduiding van een tijdsperiode met een Begindatum en/of een Einddatum.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Begindatum" type="ijw:LDT_Datum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De begindatum van de periode.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Einddatum" type="ijw:LDT_Datum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De einddatum van de periode.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Product">
		<xs:annotation>
			<xs:documentation>Gecodeerde omschrijving van een product, dienst of resultaat ten behoeve van het leveren van ondersteuning aan een client. Product wordt uitgedrukt in een categorie en eventueel nader gespecificeerd met een specifieke productcode.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Categorie" type="ijw:LDT_ProductCategorie">
				<xs:annotation>
					<xs:documentation>Gecodeerde aanduiding van hoofdcategorieen van producten in het kader van de Jeugdwet.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Code" type="ijw:LDT_ProductCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Gecodeerde aanduiding van producten in het kader van de Jeugdwet.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Referentie">
		<xs:sequence>
			<xs:element name="ReferentieNummer" type="ijw:LDT_ReferentieNummer">
				<xs:annotation>
					<xs:documentation>Uniek referentienummer dat de zorgverlener heeft toegekend aan een detailregel van de declaratie.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VorigReferentieNummer" type="ijw:LDT_ReferentieNummer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Uniek referentienummer dat de zorgverlener heeft toegekend aan een voorgaande detailregel, gerelateerd aan deze detailregel.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_RetourBedragen">
		<xs:annotation>
			<xs:documentation>Retour totaal bedragen.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="IngediendTotaalBedrag" type="ijw:CDT_TotaalBedragMetDC">
				<xs:annotation>
					<xs:documentation>Het bedrag dat in zijn totaliteit gedeclareerd of gefactureerd is.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ToegekendTotaalBedrag" type="ijw:CDT_TotaalBedragMetDC">
				<xs:annotation>
					<xs:documentation>Het totale bedrag dat de betaler toekent aan de declarant.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_RetourHeader">
		<xs:annotation>
			<xs:documentation>Retourgegevens op hoofdniveau</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="GemeenteReferentieNummer" type="ijw:LDT_GemeenteReferentieNummer">
				<xs:annotation>
					<xs:documentation>Uniek nummer waaronder de gemeente een declaratiebestand van een Wmo-aanbieder in zijn systeem heeft geregistreerd.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RetourDagtekening" type="ijw:LDT_Datum">
				<xs:annotation>
					<xs:documentation>Datum van verzending van het retourbericht.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Telefoon">
		<xs:annotation>
			<xs:documentation>Het telefoonnummer waarop de client of relatie van de client te bereiken is.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Telefoonnummer" type="ijw:LDT_Telefoonnummer">
				<xs:annotation>
					<xs:documentation>Het telefoonnummer waarop de client of relatie van de client te bereiken is.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Landnummer" type="ijw:LDT_Landnummer">
				<xs:annotation>
					<xs:documentation>Het telefoonnummer van een land, vanuit Nederland benaderd.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Telefoonnummers">
		<xs:annotation>
			<xs:documentation>De telefoonnummers waarop de client of relatie van de client te bereiken is.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Telefoon01" type="ijw:CDT_Telefoon">
				<xs:annotation>
					<xs:documentation>Het telefoonnummer waarop de client of relatie van de client te bereiken is.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Telefoon02" type="ijw:CDT_Telefoon" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Het telefoonnummer waarop de client of relatie van de client te bereiken is.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_ToegekendeBedragPrestatie">
		<xs:annotation>
			<xs:documentation>Toegekend bedrag van het ingediende bedrag.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="GemeenteBerekendBedrag" type="ijw:CDT_BedragMetDC">
				<xs:annotation>
					<xs:documentation>Dit is het bedrag dat volgens de gemeente geoorloofd is te declareren.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ToegekendBedrag" type="ijw:CDT_BedragMetDC">
				<xs:annotation>
					<xs:documentation>Het bedrag dat de betaler toekent aan de declarant.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_TotaalBedragMetDC">
		<xs:annotation>
			<xs:documentation>Samengesteld totaal bedrag met DebetCredit indicatie.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TotaalBedrag" type="ijw:LDT_TotaalBedrag">
				<xs:annotation>
					<xs:documentation>Het bedrag dat in zijn totaliteit gedeclareerd of gefactureerd, toegekend of berekend is.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DebetCredit" type="ijw:LDT_DebetCredit">
				<xs:annotation>
					<xs:documentation>Indicatie of een bedrag/aantal debet of credit is.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_VerkorteNaam">
		<xs:annotation>
			<xs:documentation>De naam van een natuurlijk persoon, aangeduid als Geslachtsnaam en eventueel Voornamen en/of Voorletters.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Geslachtsnaam" type="ijw:CDT_Achternaam">
				<xs:annotation>
